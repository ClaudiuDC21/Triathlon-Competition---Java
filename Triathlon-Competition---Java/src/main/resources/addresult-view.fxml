<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="240.0" prefWidth="320.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml"
            fx:controller="JavaProject.Controller.AddResultController">
    <children>
        <ComboBox fx:id="athleteComboBox" layoutX="85.0" layoutY="73.0" prefWidth="150.0" promptText="Participant" />
        <Label layoutX="76.0" layoutY="33.0" text="Adaugare Rezultat">
            <font>
                <Font name="System Bold" size="18.0" />
            </font>
        </Label>
        <TextField fx:id="pointsTextfield" layoutX="119.0" layoutY="120.0" prefHeight="26.0" prefWidth="83.0" promptText="Puncte" />
        <Button layoutX="165.0" layoutY="169.0" mnemonicParsing="false" onAction="#onAddButtonClicked" text="Adauga" />
        <Button layoutX="89.0" layoutY="169.0" mnemonicParsing="false" onAction="#onCloseButtonClicked" text="Anuleaza" />
    </children>
</AnchorPane>
